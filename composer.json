{
  "name": "matthew-p/yii2-services",
  "description": "Extension provide very simply use services for models and controllers",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "services",
    "controller",
    "model"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Matthew Patell",
      "email": "lukomi@mail.ru"
    }
  ],
  "require": {
    "php": "^7.1",
    "yiisoft/yii2": "^2.0.13"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "MP\\Services\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MP\\Services\\Stub\\": "stub/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  }
}
